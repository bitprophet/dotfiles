# Generated by fzf's install hook (via fzf 0.17.1)
# Then modified afterwards...

if [[ -f /usr/local/opt/fzf ]]; then

    # Auto-completion
    # ---------------
    [[ $- == *i* ]] && source "/usr/local/opt/fzf/shell/completion.zsh" 2> /dev/null

    # Key bindings
    # ------------
    source "/usr/local/opt/fzf/shell/key-bindings.zsh"

    # CUSTOM: revert ^R binding back to default zsh history search. Not convinced
    # fzf provides enough value-add there (esp as it requires an extra Enter
    # keystroke to submit the command)
    bindkey '^R' history-incremental-search-backward

fi
